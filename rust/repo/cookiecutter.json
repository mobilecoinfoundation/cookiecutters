{
  "_copy_without_render": [
      ".github/workflows/*"
  ],
  "repo_name": "repository",
  "repo_homepage": "https://mobilecoin.foundation/",
  "github_org": "mobilecoinfoundation",
  "github_teams": "coredev other_team",
  "github_username": "@your_github_username",
  "default_branch": "main",
  "owners": "{{ cookiecutter.github_username }} @jcape",
  "type": [
    "workspace",
    "single-crate"
  ],
  "license": [
    "Apache-2.0",
    "AGPL-3.0-only",
    "GPL-3.0-only"
  ],
  "arch": [
    "any",
    "x86_64",
    "sgx"
  ],
  "workspace_description": "A brief summary of the workspace",
  "workspace_readme_title": "MobileCoin: {{ cookiecutter.workspace_description }}",
  "crate_name": "mc-crate-name",
  "crate_namespace_prefix": "mc-",
  "crate_description": "A brief summary of the crate",
  "crate_readme_title": "MobileCoin: {{ cookiecutter.crate_description }}",
  "crate_sub_dir": "{{ cookiecutter.crate_name.lower().partition(cookiecutter.crate_namespace_prefix)[-1].replace('-','/') }}",
  "version": "0.1.0",
  "crate_keywords": "blockchain serde",
  "crate_categories": "cryptography no-std"
}
